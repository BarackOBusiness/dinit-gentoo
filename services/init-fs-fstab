# other non-network filesystems

type         = scripted
command      = /usr/libexec/dinit/early/fs-fstab.sh start
stop-command = /usr/libexec/dinit/early/fs-fstab.sh stop
depends-on   = init-fs-pre.target
depends-on   = init-root-rw.target
depends-on   = init-fs-zfs
depends-on   = init-fs-btrfs
depends-ms   = init-fs-fsck
restart      = false
